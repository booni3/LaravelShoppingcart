{
    "name": "ollywarren/shoppingcart",
    "description": "Laravel 5.5+ Shopping Cart Package based on the Original Rob Gloudemans Package",
    "keywords": ["laravel", "shoppingcart", "shipping", "discount codes"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        },
        {
            "name": "Olly Warren",
            "email": "olly@ollywarren.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.5",
        "illuminate/session": "^5.5",
        "illuminate/events": "^5.5",
        "php-units-of-measure/php-units-of-measure": "^2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Ollywarren\\ShoppingCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ollywarren\\Tests\\ShoppingCart\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Ollywarren\\ShoppingCart\\ShoppingCartServiceProvider"
            ],
            "aliases": {
                "Cart": "Ollywarren\\ShoppingCart\\Facades\\Cart"
            }
        }
    }
}
